# production settings
config {
    no_cache = 0
    contentObjectExceptionHandler = 1

    # activate compression
    compressJs = 1
    concatenateJs = 1
    compressCss = 1
    concatenateCss = 1

    sendCacheHeaders = 1

    # disable admin panel
    admPanel = 0
}

